{
  "name": "frec-front",
  "version": "1.0.0",
  "description": "My first-rate Nuxt.js project",
  "author": "pt-suzuki",
  "private": true,
  "scripts": {
    "dev": "cross-env NUXT_HOST=0.0.0.0 NODE_ENV=development nuxt-ts",
    "build": "nuxt-ts build",
    "dev-build": "cross-env NODE_ENV=dev nuxt-ts build",
    "prod-build": "cross-env NODE_ENV=production nuxt-ts build",
    "dev-deploy": "npm run dev-build && sls deploy --stage dev",
    "staging-deploy": "npm run build && sls deploy --stage staging",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .js,.ts,.vue ./src && stylelint ./src/**/*.vue ./src/**/*.css /src/**/*.scss",
    "lint:fix": "eslint --ext .js,.ts,.vue ./src --fix && stylelint ./src/**/*.vue /src/**/*.css /src/**/*.scss --fix",
    "lint:ts": "eslint --ext .js,.ts,.vue ./src",
    "lint:css": "stylelint ./src/**/*.vue ./src/**/*.css ./src/**/*.scss",
    "lint:ts:fix": "eslint --ext .js,.ts,.vue ./src --fix",
    "lint:css:fix": "stylelint ./src --fix",
    "test": "jest"
  },
  "dependencies": {
    "@nuxt/types": "2.14.1",
    "@nuxt/typescript-runtime": "1.0.0",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/eslint-config-typescript": "3.0.0",
    "@nuxtjs/eslint-module": "2.0.0",
    "axios": "0.19.2",
    "cross-env": "7.0.2",
    "dotenv": "8.2.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-babel-module-alias": "1.5.1",
    "eslint-import-resolver-webpack": "0.12.2",
    "eslint-plugin-unicorn": "21.0.0",
    "nuxt": "2.14.1",
    "skyway-js": "^3.0.2",
    "vue": "2.6.11",
    "vue-class-component": "7.2.5",
    "vue-client-only": "2.0.0",
    "vue-loading-overlay": "3.3.3",
    "vue-loading-spinner": "1.0.11",
    "vue-property-decorator": "9.0.0",
    "vue-server-renderer": "2.6.11",
    "vue-spinner": "1.0.4",
    "vue-template-compiler": "2.6.11",
    "vuex": "3.5.1",
    "vuex-module-decorators": "0.17.0",
    "which": "2.0.2"
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "5.14.0",
    "@nuxt/types": "2.14.1",
    "@nuxt/typescript-build": "2.0.2",
    "@nuxtjs/eslint-config": "3.1.0",
    "@nuxtjs/font-awesome": "1.0.4",
    "@nuxtjs/style-resources": "1.0.0",
    "@nuxtjs/toast": "3.3.1",
    "@types/jest": "25.2.3",
    "@typescript-eslint/eslint-plugin": "3.9.0",
    "@typescript-eslint/parser": "3.9.0",
    "@vue/test-utils": "1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-preset-vue": "2.0.2",
    "concurrent-transform": "1.0.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "13.0.1",
    "eslint-import-resolver-babel-module": "5.1.2",
    "eslint-loader": "2.2.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-nuxt": "1.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "6.2.2",
    "eslint-utils": "2.1.0",
    "gulp": "4.0.2",
    "gulp-awspublish": "4.1.2",
    "gulp-cloudfront-invalidate-aws-publish": "1.0.0",
    "husky": "4.2.5",
    "jest": "26.4.0",
    "node-sass": "4.14.1",
    "nodemon": "2.0.4",
    "serverless-layers": "2.2.4",
    "sass-loader": "9.0.3",
    "stylelint": "13.6.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.18.0",
    "stylelint-webpack-plugin": "2.1.0",
    "ts-jest": "26.2.0",
    "typescript": "3.9.7",
    "underscore": "1.10.2",
    "vue-jest": "4.0.0-beta.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "vue",
      "json"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test/**/*.+(spec|test).[tj]s?(x)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/$1",
      "^vue$": "vue/dist/vue.common.js"
    },
    "preset": "ts-jest",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/components/**/*.vue",
      "<rootDir>/pages/**/*.vue"
    ]
  }
}
